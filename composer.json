{
  "name": "aarif12345/imagerepository",
  "autoload": {
    "psr-4": {
      "ImageRepository\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "files": [
      "src/Utils/constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ImageRepository\\Tests\\": "tests/"
    }
  },
  "keywords": [
    "pdo",
    "mysql",
    "database",
    "iterator"
  ],
  "description": "A API for the Image repository created for the Shopify challenge",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Arif",
      "email": "abdullahmeo11@gmail.com"
    }
  ],
  "require": {
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^7.0",
    "phpauth/phpauth": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}